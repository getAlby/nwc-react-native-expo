{
  "expo": {
    "name": "NWC React native",
    "slug": "nwc-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.nwc_react_native",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.example.nwc_react_native",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {}
    },
    "owner": "roland_alby",
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
